{
  "version": 2,
  "buildCommand": "cd frontend && npm install && npm run db:generate && npm run build",
  "outputDirectory": "frontend/.next",
  "installCommand": "cd frontend && npm install",
  "functions": {
    "frontend/pages/api/**/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "env": {
    "DATABASE_URL": "@database_url"
  },
  "github": {
    "enabled": true,
    "autoAlias": false
  }
}